<view class="content"><view class="Scan"><view class="inputWrapper"><label class="label">物料条码:</label><input class="input" focus="{{labelFoucs}}" placeholder="请输入物料条码" data-event-opts="{{[['confirm',[['OnEnter']]],['input',[['__set_model',['','ScanMaterialLabel','$event',[]]]]]]}}" value="{{ScanMaterialLabel}}" bindconfirm="__e" bindinput="__e"/></view><view class="inputWrapper"><label class="label">库位编码:</label><input class="input" focus="{{locationFoucs}}" placeholder="请输入库位码" data-event-opts="{{[['input',[['__set_model',['$0','LocationCode','$event',[]],['LabelEntity']]]]]}}" value="{{LabelEntity.LocationCode}}" bindinput="__e"/></view><view class="btn"><button class="btnValue" type="primary" disabled="{{IsCanStartOrder==false}}" size="mini" data-event-opts="{{[['tap',[['ScanKeyDown']]]]}}" bindtap="__e">开始任务</button><button class="btnValue" type="primary" disabled="{{IsCanSkipLocation==false}}" size="mini" data-event-opts="{{[['tap',[['SkipCurrentLocation']]]]}}" bindtap="__e">切换库位</button></view></view><view class="Scan"><view><uni-card class="card" vue-id="5be7652a-1" title="条码信息" bind:__l="__l" vue-slots="{{['default']}}"><view><view class="inputWrapper"><label class="label">物料条码:</label><text class="label">{{this.LabelEntity.Code}}</text></view><view class="inputWrapper"><text class="label">物料编码:</text><text class="label">{{this.LabelEntity.MaterialCode}}</text></view><view class="inputWrapper"><label class="label">物料数量:</label><input class="input" type="number" placeholder="请输入数量" data-event-opts="{{[['input',[['__set_model',['$0','Quantity','$event',[]],['LabelEntity']]]]]}}" value="{{LabelEntity.Quantity}}" bindinput="__e"/></view><view class="inputWrapper"><text class="label">批号:</text><text class="label">{{this.LabelEntity.Batchcode}}</text></view><view class="inputWrapper"><text class="label">生产日期:</text><text class="label" style="width:65%;">{{this.LabelEntity.ProductionDate}}</text></view></view></uni-card></view></view><view class="Scan btn"><button class="btnValue" type="primary" size="mini" data-event-opts="{{[['tap',[['ConfirmShelf']]]]}}" bindtap="__e">上架</button><button class="btnValue" type="primary" size="mini" data-event-opts="{{[['tap',[['FinisheCurrentOrder']]]]}}" bindtap="__e">完成</button></view></view>